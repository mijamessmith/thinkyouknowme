  <link rel="stylesheet" type="text/css" href="/login.css">
    <div class="login-container">
      <div class="inner-container">
        <div class="row">
          <div class="col-md-4 col-md-offset-4">
            <div class="form-body">
              <ul class="nav nav-tabs final-login">
                <li class="active">
                  <a data-toggle="tab" href="#sectionA">Sign In</a>
                </li>
                <li>
                  <a data-toggle="tab" href="#sectionB">Join the Community</a>
                </li>
              </ul>
              <div class="tab-content">
                <div id="sectionA" class="tab-pane fade in active">
                  <div class="innter-form">
                    <form action="/logUserIn" method="post" class="sa-innate-form">
                      <label>Email Address</label>
                      <input type="text" name="email">
                        <label>Password</label>
                        <input type="password" name="password">
                          <button type="submit">Sign In</button>
                          <div class="password-container">
                            <%var message=""%>
                              <h5>
                                <%- message %>
                              </h5>
                            </div>
                          <a href="">Forgot Password?</a>
                        </form>
                  </div>

                  <div class="clearfix"></div>
                </div>
                <div id="sectionB" class="tab-pane fade">
                  <div class="innter-form">
                    <form id="registerForm" class="sa-innate-form" action="/register" method="post">
                      <label>First Name</label>
                      <input type="text" name="firstName">
                        <label>Last Name</label>
                        <input type="text" name="lastName">
                          <label>Email Address</label>
                          <input type="text" name="email">
                            <label>Password</label>
                            <input type="password" name="password">
                              <button type="submit" id="signup">Join now</button>
                              <p>By clicking Join now, you agree to Think You Know Me's User Agreement, Privacy Policy, and Cookie Policy.</p>
                            </form>
                  </div>
                  <div class="container">
                    <h5>
                      <%=message%>
                    </h5>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
<script src="./scripts/login-script.js"></script>
