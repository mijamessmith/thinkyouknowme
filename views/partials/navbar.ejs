    console.log("Landing Btn has been clicked")
 
    let ajaxReq; 
    ajaxReq = $.ajax({
    url: '/', 
  type: 'GET',
  datatype: "json",
  data: "" //we're not sending
  });

  ajaxReq.done(function (data) {
  
  if (data !== undefined) {
  window.location.replace(`http://localhost:3000/`); //sending a new addresss to load
  }
  });
  });

  logout.addEventListener("click", function (event) {
  event.preventDefault()
  console.log("Logout Btn has been clicked")

  let ajaxReq; //sometimes declaring and initializing has issues
  ajaxReq = $.ajax({
  url: '/logout',
  type: 'GET',
  datatype: "json",
  data: "" //we're not sending
